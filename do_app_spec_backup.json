{
  "name": "transcripcion-audio-app",
  "services": [
    {
      "name": "transcripci-n-de-audio",
      "image": {
        "registry_type": "DOCR",
        "registry": "transcriptor",
        "repository": "transcripcion-audio",
        "tag": "full-latest"
      },
      "run_command": "python main.py",
      "environment_slug": "python",
      "envs": [
        {
          "key": "WHISPER_MODEL",
          "value": "small",
          "scope": "RUN_AND_BUILD_TIME"
        },
        {
          "key": "MAX_FILE_SIZE",
          "value": "209715200",
          "scope": "RUN_AND_BUILD_TIME"
        }
      ],
      "instance_size_slug": "basic-xxs",
      "instance_count": 1,
      "http_port": 8888
    }
  ],
  "region": "nyc",
  "ingress": {
    "rules": [
      {
        "match": {
          "path": {
            "prefix": "/"
          }
        },
        "component": {
          "name": "transcripci-n-de-audio"
        }
      }
    ]
  }
}
